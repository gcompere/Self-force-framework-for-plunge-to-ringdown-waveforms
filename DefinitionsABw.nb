Cell[BoxData[
 RowBox[{
  RowBox[{"FindM\[Omega]QNM", "[", 
   RowBox[{"s_", ",", "l_", ",", "seedM\[Omega]_", ",", "setprecision_"}], 
   "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
     "w", ",", "c0", ",", "c1", ",", "c2", ",", "c3", ",", "c4", ",", 
      "\[Gamma]", ",", "\[Beta]", ",", "\[Alpha]", ",", "n", ",", "Nmax", ",",
       "InfFact", ",", "ZEROR", ",", "PrecisionIncreaseFactor"}], "}"}], ",", 
    "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{"setprecision", "==", "1"}], ",", 
       RowBox[{
        RowBox[{"Nmax", "=", "1500"}], ";", 
        RowBox[{"PrecisionIncreaseFactor", "=", "35"}], ";"}]}], "]"}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{"setprecision", "==", "2"}], ",", 
       RowBox[{
        RowBox[{"Nmax", "=", "300"}], ";", 
        RowBox[{"PrecisionIncreaseFactor", "=", "10"}], ";"}]}], "]"}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{"setprecision", "==", "3"}], ",", 
       RowBox[{
