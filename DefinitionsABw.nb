        RowBox[{"PrecisionIncreaseFactor", "=", "20"}], ";"}]}], "]"}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"c0", "=", 
      RowBox[{"N", "[", 
       RowBox[{
        RowBox[{"1", "-", "s", "-", 
         RowBox[{"2", "I", " ", "w"}]}], ",", 
        RowBox[{"PrecisionIncreaseFactor", " ", "MachinePrecision"}]}], 
       "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"c1", "=", 
      RowBox[{"N", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"-", "4"}], "+", 
         RowBox[{"8", "I", " ", "w"}]}], ",", 
        RowBox[{"PrecisionIncreaseFactor", " ", "MachinePrecision"}]}], 
       "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"c2", "=", 
      RowBox[{"N", "[", 
